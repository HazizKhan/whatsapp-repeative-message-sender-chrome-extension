{
  "manifest_version": 3,
  "name": "Send whatsapp message",
  "description": "Send whatsapp message.",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "hello.html"
  },
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
